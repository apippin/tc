digraph schema {
  graph [
    rankdir = "LR"
  ];
  activity   [
    label = "activity||<a> activity|<s> assignment|<t> date|<n> notes"
    shape = "record"
  ];
  appointment   [
    label = "appointment||<a> appointment|<p> presidency|<f> family|<i> individual |<d> date|<t> time|<l> location|<u> uid"
    shape = "record"
  ];
